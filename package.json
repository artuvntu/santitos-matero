{
  "name": "santitos-pos",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng serve --port=8080",
    "start": "ng serve & electron .",
    "build": "ng build --baseHref=./",
    "build:prod": "ng build --prod --baseHref=/santitos-pos/",
    "build:release": "ng build --prod --build-optimizer --baseHref=/santitos-pos/",
    "test": "ng test",
    "lint": "npm run lint:ts & npm run lint:style",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:scss": "stylelint --syntax scss \"src/**/*.scss\" --fix",
    "e2e": "ng e2e",
    "hmr": "ng serve -c=hmr --disable-host-check",
    "deploy": "angular-cli-ghpages --no-silent --dir=dist/santitos-pos",
    "electron": "electron .",
    "electron:build": "ng build --prod --baseHref=./ && electron .",
    "electron-package": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "electron-package:win": "electron-packager . electron-tutorial-app --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.3",
    "@angular/cdk": "^8.0.2",
    "@angular/common": "~8.0.3",
    "@angular/compiler": "~8.0.3",
    "@angular/core": "~8.0.3",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.0.3",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "~8.0.3",
    "@angular/platform-browser-dynamic": "~8.0.3",
    "@angular/router": "~8.0.3",
    "@antv/data-set": "^0.10.2",
    "@antv/g2": "^3.5.7",
    "@antv/g2-plugin-slider": "^2.1.1",
    "@ng-select/ng-select": "^2.20.3",
    "@ngx-formly/core": "^5.0.0",
    "@ngx-formly/material": "^5.0.0",
    "@ngx-progressbar/core": "^5.3.2",
    "@ngx-progressbar/router": "^5.3.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "async": "^3.1.0",
    "core-js": "^3.1.4",
    "electron": "^11.5.0",
    "hammerjs": "^2.0.8",
    "mongoose": "^5.6.10",
    "mongoose-unique-validator": "^2.0.3",
    "ngx-electron": "^2.1.1",
    "ngx-popper": "^6.2.0",
    "ngx-toastr": "^10.0.4",
    "node-thermal-printer": "^4.1.0",
    "photoviewer": "3.3.0",
    "popper.js": "^1.15.0",
    "rxjs": "~6.5.2",
    "screenfull": "^4.2.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/cli": "~8.0.6",
    "@angular/compiler-cli": "~8.0.3",
    "@angular/language-service": "~8.0.3",
    "@angularclass/hmr": "^2.1.3",
    "@ngx-formly/schematics": "^5.3.0",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.0.10",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "1.18.2",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.4.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.9.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.4.5"
  }
}
